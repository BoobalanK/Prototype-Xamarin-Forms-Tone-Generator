<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.MainPage">

   <ContentPage.Content>
      <StackLayout>
         <Button Text="Tone Types" Clicked="ToneTypes_Clicked"/>
         <Button Text="WebView"
                 Clicked="WebView_Clicked" />
         <Label Text="Android Service" BackgroundColor="DodgerBlue" FontSize="Medium" TextColor="White"/>
         <Button Text="Start Service"
                 Clicked="StartServiceClicked" />
         <Button Text="Stop Service"
                 Clicked="StopServiceClicked" />
      </StackLayout>
   </ContentPage.Content>

</ContentPage>
